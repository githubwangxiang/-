<script>
	$(function(){
		//捕捉点击事件
		$('.mnunss').click(function(){
			$(this).removeAttr('class');
			$(this).addClass('cur');
		
		var res_city = $('.mnunss').closest('ul').find('a').attr('food');
		console.log(res_city);
		//发送ajax请求
		$.ajax({
			'url':"__CONTROLLER__/foodcity",
			'type':'post',
			'data':res_city,
			'dataType':'json',
			'success':function(response){
				console.log(response);
				if(response.code != 10000){
					alert(response.msg);
					return ;
				}else{
					$('.title').closest('div').find('a').html(response.str);
				}
			}
		})
	})
	});
</script>


